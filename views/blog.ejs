<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% allPosts.forEach(function(p) { %>
        <tr>
            <!-- <td><%= p.image %></td> -->
          <li>
            <td><%= p.title %></td>
            <td><%= p.author %></td>
            <td><%= p.body %></td>
            <td><%= p.postedOn %></td>
        </li>
        </tr>
    <% }); %><br><br>
</body>
</html>